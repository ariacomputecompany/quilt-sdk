{
  "name": "quilt-sdk",
  "version": "2.0.0",
  "description": "TypeScript SDK and CLI for Quilt container runtime",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "quilt": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:binary": "bun build src/cli.ts --compile --outfile dist/quilt",
    "build:binary:linux-x64": "bun build src/cli.ts --compile --target=bun-linux-x64 --outfile dist/quilt-linux-x64",
    "build:binary:linux-arm64": "bun build src/cli.ts --compile --target=bun-linux-arm64 --outfile dist/quilt-linux-arm64",
    "build:binary:darwin-x64": "bun build src/cli.ts --compile --target=bun-darwin-x64 --outfile dist/quilt-darwin-x64",
    "build:binary:darwin-arm64": "bun build src/cli.ts --compile --target=bun-darwin-arm64 --outfile dist/quilt-darwin-arm64",
    "build:binaries": "npm run build:binary:linux-x64 && npm run build:binary:linux-arm64 && npm run build:binary:darwin-x64 && npm run build:binary:darwin-arm64",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "test": "node examples/basic.js"
  },
  "keywords": [
    "quilt",
    "containers",
    "cli",
    "http",
    "rest",
    "typescript",
    "sdk",
    "ai-agents"
  ],
  "author": "Quilt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ariacomputecompany/quilt-sdk.git"
  },
  "homepage": "https://quilt.sh",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "spec.json"
  ]
}
